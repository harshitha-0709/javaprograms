import java.util.Scanner;
class HelloWorld {
    public static void main(String[] args) {
       Scanner sc=new Scanner(System.in);
       int n=sc.nextInt();
       for(int i=n;i>0;i--){
           for(int k=n-i+1,j=(k)*(k-1)/2+1;j<=(k)*(k+1)/2;j++)
           {
               System.out.print(j+" ");
           }
           System.out.println();
       }
}
}
